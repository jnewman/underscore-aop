{
    "browser": true,
    "curly": true,
    "devel": true,
    "indent": 4,
    "latedef": true,
    "maxcomplexity": 10,
    "maxdepth": 6,
    "maxlen": 100,
    "maxparams": 100,
    "maxstatements": 40,
    "node": true,
    "newcap": true,
    "nonew": true,
    "predef": [
        "after",
        "afterEach",
        "before",
        "beforeEach",
        "define",
        "describe",
        "it",
        "require"
    ],
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": "vars"
}
