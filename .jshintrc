{
    "browser": true,
    "curly": true,
    "devel": true,
    "indent": 4,
    "latedef": true,
    "maxcomplexity": 10,
    "maxdepth": 6,
    "maxlen": 80,
    "maxparams": 4,
    "maxstatements": 40,
    "newcap": true,
    "noempty": true,
    "nonew": true,
    "predef": [
        "define",
        "require",
        "mocha"
    ],
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
}
